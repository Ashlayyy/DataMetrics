{
  "name": "archie_dashboard_api",
  "version": "1.0.0",
  "description": "The API For The Archie Dashboard",
  "main": "dist/index.js",
  "scripts": {
    "test": "concurrently \"cross-env NODE_ENV=testing jest --detectOpenHandles\" \"npm:dev\"",
    "clean": "rimraf ./dist && rimraf ./coverage && npm run clean-install",
    "code:check": "concurrently \"npm:prettier:check\" \"npm:lint\" \"npm:audit\"",
    "code:clean": "concurrently \"npm:prettier:write\" \"npm:lint:fix\"",
    "build": "npm run clean && npm run audit && npm run code:clean && tsc",
    "start": "cross-env NODE_ENV=production node ./dist/src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "debug": "cross-env NODE_ENV=development DEBUG=* nodemon ./src/server.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:check": "prettier ./src --check",
    "prettier:write": "prettier ./src --write",
    "audit": "better-npm-audit audit",
    "clean-install": "rimraf ./package-lock.json && rimraf ./node_modules && npm install",
    "build-start": "npm run build && npm run start"
  },
  "author": "Ashlay Steur",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "apicache": "^1.6.3",
    "axios": "^1.7.2",
    "better-npm-audit": "^3.7.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^9.0.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.2.0",
    "express-slow-down": "^2.0.3",
    "helmet": "^7.0.0",
    "http-status": "^1.7.4",
    "luxon": "^3.4.4",
    "mongoose": "^8.4.4",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rotating-file-stream": "^3.2.1",
    "sequelize": "^6.37.3",
    "swagger-ui-express": "^5.0.1",
    "tedious": "^18.2.1",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.6.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^4.17.13",
    "@types/express-slow-down": "^1.3.2",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.8",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/parser": "^7.14.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
